<%= form_for @book do |f| %>
<fieldset>
  <%= f.label :title %>
  <%= f.text_field :title %>
</fieldset>

<fieldset>
  <%= f.label :generates %>
  <%= f.text_field :generates %>
</fieldset>
<fieldset>
  <%= f.label :year %>
  <%= f.text_field :year %>
</authors_id>
<fieldset>
  <%= f.label :image %>
  <%= f.url_field :title %>
</fieldset>

<fieldset>
  <%= f.label :author_id %>
  <%= f.select :author_id, Author.pluck(:name, :id), :include_blank => true %>
</fieldset>

<%= f.submit %>
<% end %>
