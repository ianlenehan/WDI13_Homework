<h2>Edit Logo</h2>

<form action="/logos/<%= @logo.id %>" method="POST">
  <fieldset class="form-group">
  <p><label>Origin: </label><input class="form-control" name="origin" value="<%= @logo.origin %>"></p>
  <p><label>Year: </label><input class="form-control" name="year" value="<%= @logo.year %>"></p>
  <p><label>Image:</label><input class="form-control" type="text" name="image" value="<%= @logo.image %>"></p>
  <p><label>More Information:</label><input class="form-control" name="more_info" value="<%= @logo.more_info %>"></p>
  <p><input class="btn btn-default submit-button" type="submit" value="Edit Logo"></P>
</fieldset>
</form>
